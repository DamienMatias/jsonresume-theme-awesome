{{#resume.basics}}
<header class="resume-header clearfix">
    <div class="profile-pic pull-left">
        {{#if image}}
        <img src="{{image}}" alt="profile-pic">
        {{/if}}
        {{#if picture}}
        <img src="{{picture}}" alt="profile-pic">
        {{/if}}
    </div>
    <div class="profile-header pull-left">
        <h1>{{name}}</h1>
        <h2>{{label}}</h2>
    </div>
    <div class="profile-about pull-right">
        {{> about }}
    </div>
</header>
{{/resume.basics}}